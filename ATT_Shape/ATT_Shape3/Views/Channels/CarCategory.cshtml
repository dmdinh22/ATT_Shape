
@{
	Layout = "~/Views/Shared/_LayoutChannelCategories.cshtml";
}

@section scripts
{
	<script type="text/javascript">
		(function () {
			"use strict";

			angular.module("attApp")
				.controller('channelCategoryController', channelCategoryController);

			channelCategoryController.$inject = ['$scope', '$sce'];

			function channelCategoryController($scope, $sce) {

				var vm = this;
				vm.category = 'Car';
				vm.channels = [{ url: "http://www.motorsportnationals.com/wp-content/uploads/2015/09/engine-tips.jpg", title: "EngineBuilding" }, { url: "http://pop.h-cdn.co/assets/cm/15/05/54cae4238a5cf_-_american-muscle-facts-05-0312-xln.jpg", title: "MuscleCars" }, { url: "https://media-cdn.tripadvisor.com/media/photo-s/07/ca/b3/92/dream-racing.jpg", title: "Racing" }, ]
				vm.$scope = $scope;
				vm.showNews = false;
				render();

				function render() {
					console.log("channel page startup");
				}
			};

		})();

	</script>
}



